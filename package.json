{
  "name": "emotion",
  "version": "0.0.2",
  "description": "Emotion, 2motion.",
  "author": "a@hax0r.info",
  "license": "MIT",
  "scripts": {
    "release": "standard-version",
    "prebuild": "rimraf dist",
    "build": "./node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "./node_modules/.bin/nest start",
    "start:dev": "./node_modules/.bin/nest start --watch",
    "start:debug": "./node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "./node_modules/.bin/sls deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^6.8.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^6.8.0",
    "@nestjs/platform-express": "^6.8.0",
    "@nestjs/sequelize": "^0.1.0",
    "aws-serverless-express": "^3.3.5",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "nest-emitter": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sequelize": "^5.21.7",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@nestjs/cli": "^6.9.1",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/testing": "^6.8.0",
    "@types/aws-lambda": "^8.10.40",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^12.7.8",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "lint-staged": "^10.2.2",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "serverless": "^1.69.0",
    "serverless-offline": "^4.9.2",
    "serverless-plugin-optimize": "^4.1.4-rc.1",
    "serverless-plugin-warmup": "^4.9.0",
    "standard-version": "^7.1.0",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
