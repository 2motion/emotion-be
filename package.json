{
  "name": "emotion",
  "version": "0.0.2",
  "description": "Emotion, 2motion.",
  "author": "a@hax0r.info",
  "license": "MIT",
  "scripts": {
    "release": "standard-version",
    "prebuild": "rimraf dist",
    "build": "./node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "./node_modules/.bin/nest start",
    "start:dev": "./node_modules/.bin/nest start --watch",
    "start:debug": "./node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage && coveralls < coverage/lcov.info",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "./node_modules/.bin/sls deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "7.1.0",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.1.0",
    "@nestjs/platform-express": "7.1.0",
    "@nestjs/sequelize": "0.1.0",
    "@nestjs/swagger": "4.5.6",
    "@sentry/node": "5.15.5",
    "@slack/webhook": "5.0.3",
    "aws-sdk": "2.683.0",
    "aws-serverless-express": "3.3.8",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "google-libphonenumber": "3.2.10",
    "gravatar": "^1.8.0",
    "helmet": "3.22.0",
    "jsonwebtoken": "8.5.1",
    "mime-types": "2.1.27",
    "moment": "2.26.0",
    "multer": "1.4.2",
    "mysql2": "2.1.0",
    "nest-emitter": "1.1.0",
    "nest-raven": "5.0.0",
    "reflect-metadata": "0.1.13",
    "request-ip": "2.1.3",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "sequelize": "5.21.9",
    "sequelize-typescript": "1.1.0",
    "swagger-ui-express": "4.1.4",
    "tinify": "1.6.0-beta.2"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-angular": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@nestjs/cli": "7.1.5",
    "@nestjs/schematics": "7.2.7",
    "@nestjs/testing": "7.1.0",
    "@types/aws-lambda": "8.10.71",
    "@types/aws-serverless-express": "3.3.3",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.7",
    "@types/google-libphonenumber": "7.4.18",
    "@types/gravatar": "^1.8.1",
    "@types/helmet": "0.0.48",
    "@types/jest": "26.0.15",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mime-types": "2.1.0",
    "@types/moment": "2.13.0",
    "@types/multer": "1.4.4",
    "@types/node": "12.12.37",
    "@types/request-ip": "0.0.35",
    "@types/sequelize": "4.28.9",
    "@types/supertest": "2.0.10",
    "@types/uuid": "8.0.1",
    "coveralls": "3.1.0",
    "eslint": "7.15.0",
    "husky": "4.3.0",
    "jest": "26.6.2",
    "lint-staged": "10.4.0",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "serverless": "1.79.0",
    "serverless-domain-manager": "4.2.0",
    "serverless-offline": "6.8.0",
    "serverless-plugin-optimize": "4.1.4-rc.1",
    "standard-version": "8.0.2",
    "supertest": "4.0.2",
    "ts-jest": "26.4.3",
    "ts-loader": "8.0.5",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.9.5"
  },
  "jest": {
    "moduleNameMapper": {
      "^@app/(.*)": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
