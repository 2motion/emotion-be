{
  "name": "emotion",
  "version": "0.0.2",
  "description": "Emotion, 2motion.",
  "author": "a@hax0r.info",
  "license": "MIT",
  "scripts": {
    "release": "standard-version",
    "prebuild": "rimraf dist",
    "build": "./node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "./node_modules/.bin/nest start",
    "start:dev": "./node_modules/.bin/nest start --watch",
    "start:debug": "./node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage && coveralls < coverage/lcov.info",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "./node_modules/.bin/sls deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "6.11.11",
    "@nestjs/config": "0.4.0",
    "@nestjs/core": "6.11.11",
    "@nestjs/platform-express": "6.11.11",
    "@nestjs/sequelize": "0.1.0",
    "aws-serverless-express": "3.3.8",
    "bcrypt": "4.0.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "jsonwebtoken": "8.5.1",
    "mysql2": "2.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "sequelize": "5.21.7",
    "sequelize-typescript": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "@nestjs/cli": "6.14.2",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/swagger": "4.5.3",
    "@nestjs/testing": "6.11.11",
    "@types/aws-lambda": "8.10.51",
    "@types/bcrypt": "3.0.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/jsonwebtoken": "8.3.9",
    "@types/node": "12.12.37",
    "@types/sequelize": "4.28.8",
    "@types/supertest": "2.0.9",
    "coveralls": "^3.1.0",
    "eslint": "6.8.0",
    "husky": "4.2.5",
    "jest": "25.5.4",
    "lint-staged": "10.2.2",
    "nodemon": "2.0.3",
    "prettier": "1.19.1",
    "serverless": "1.69.0",
    "serverless-offline": "4.10.6",
    "serverless-plugin-optimize": "4.1.4-rc.1",
    "serverless-plugin-warmup": "4.9.0",
    "standard-version": "7.1.0",
    "supertest": "4.0.2",
    "swagger-ui-express": "4.1.4",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.2",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^@app/(.*)": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
